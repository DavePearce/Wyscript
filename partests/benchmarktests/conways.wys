						count = count + 1
				if (y+1<gridY):
					if (grid[y+1][x] == 1):
						count = count + 1
				if (x+1<gridX):
					if (grid[y][x+1]==1):
							count = count + 1
					if (y+1 < gridY):
						if (grid[y+1][x+1]==1):
							count = count + 1
					if (y-1>=0):
						if (grid[y-1][x+1]==1):
							count = count + 1
				if (count < 2):
					newgrid[y][x] = 0
				else:
