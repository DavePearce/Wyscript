	print "running conway's\n"
	while (n < iterations):
		for x in 0..gridX :
			for y in 0..gridY:
//				newgrid[y][x] = 0
				int count = 0
				if (x-1>=0):
					if (grid[y][x-1]==1):
							count = count + 1
					if (y+1 < gridY):
						if (grid[y+1][x-1] == 1):
							count = count + 1
					if (y-1>=0):
