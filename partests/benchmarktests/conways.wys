						newgrid[y][x] = 0
					else:
						if ((count == 3)):
							newgrid[y][x] = 1
						if (count==2 && grid[y][x] == 1):
							newgrid[y][x] = 1
		tempgrid = grid
		grid = newgrid
		newgrid = tempgrid
		print n
		n = n + 1
	return grid

