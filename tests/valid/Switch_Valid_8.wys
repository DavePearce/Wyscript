constant Red is 1
constant Blue is 2
constant Green is 3

function f(int c) => int:
    switch(c):
        case Red:
            return 123
        case Blue:
            return 234
        default:
            return 456

function main() => void:
    print "NUM: " ++ f(Red)
    print "NUM: " ++ f(Green)
    print "NUM: " ++ f(Blue)