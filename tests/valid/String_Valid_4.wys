function indexOf(char c1, string str) => int|null:
	for j in 0 .. |str|:
		if (str[j] == c1):
			return j
	return null

function main() => void:
    print indexOf('H', "Hello World")
    print indexOf('e', "Hello World")
    print indexOf('l', "Hello World")
    print indexOf('o', "Hello World")
    print indexOf(' ', "Hello World")
    print indexOf('W', "Hello World")
    print indexOf('r', "Hello World")
    print indexOf('d', "Hello World")
    print indexOf('z', "Hello World")
    print indexOf('1', "Hello World")

