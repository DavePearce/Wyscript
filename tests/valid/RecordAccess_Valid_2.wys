type IntPoint is {int y, int x}

type RealPoint is {real y, real x}

function sum1(IntPoint vp) => int:
    return vp.x + vp.y

function sum2(RealPoint vp) => real:
    return vp.x + vp.y

function main() => void:
    IntPoint ip = {x: 1, y: 2}
    print sum1(ip)
    RealPoint rp = {x: 1.23, y: 2.34}
    print sum2(rp)

