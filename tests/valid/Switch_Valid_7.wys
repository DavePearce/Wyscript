function f(string input) => string:
    string r
    char c = input[0]
    switch(c):
        case 'N':
            r = "GOT KNIGHT"
        case 'B':
            r = "GOT BISHOP"
        case 'R':
            r = "GOT ROOK"
        case 'Q':
            r = "GOT QUEEN"
        case 'K':
            r = "GOT KING"
        default:
            r = "GOT NOTHING"
    return r

function main() => void:
    print f("N")
    print f("K")
    print f("Q")
    print f("B")
    print f("R")
    print f("Q")
    print f("e")
    print f("1")
