function has(char c1, string str) => bool:
     for i in 0..|str|:
        if str[i] == c1:
            return true
     return false

function main() => void:
    string s = "Hello World"
    print has('l', s)
    print has('e', s)
    print has('h', s)
    print has('z', s)
    print has('H', s)

